set output outdir."/keyword-dropping.tex"
plot indir."/keyword-dropping.dat" using 1:2:xtic(1) title "GNNK-BF (MAX)" with linespoints, \
	'' using 1:3:xtic(1) title "GNNK-BF (SUM)" with linespoints
